[server]

[mysqld]
bind-address=0.0.0.0
datadir=/mariadb/var
tmpdir=/mariadb/var/tmp
pid-file=/run/mariadb/mariadbd.pid
socket=/run/mariadb/mariadbd.sock
collation-server = utf8mb4_unicode_ci
init-connect = 'SET NAMES utf8mb4'
character-set-server = utf8mb4
binlog_format=ROW
log-bin=ON
default-storage-engine=InnoDB
innodb_autoinc_lock_mode=2

[galera]

[sst]

[embedded]

[mariadb]

[mariadb-10.5]